{
  "name": "@kundinos/react-hooks",
  "version": "0.5.9",
  "description": "Set of useful React hooks",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "compile": "rollup -c rollup.config.ts",
    "lint": "eslint src/ --ext '.ts, .tsx'",
    "lint:write": "eslint src/ --fix --ext '.ts, .tsx'",
    "pretty": "prettier --write src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kundinos/react-hooks.git"
  },
  "keywords": [
    "react",
    "hooks"
  ],
  "author": {
    "name": "Yury Kundin",
    "email": "yury.kundin@yandex.ru"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kundinos/react-hooks/issues"
  },
  "homepage": "https://github.com/kundinos/react-hooks#readme",
  "files": [
    "src",
    "dist",
    "!dist/*.test.*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{json,md,yml}": [
      "npm run pretty",
      "git add"
    ],
    "*.(ts|tsx)": [
      "npm run pretty",
      "npm run lint:write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "@babel/runtime": "^7.13.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.13.8",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/jest": "^26.0.15",
    "@types/react-dom": "^17.0.8",
    "@types/rollup-plugin-peer-deps-external": "^2.2.0",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.0.1",
    "prettier": "^2.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.40.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {}
}
