    "lint:write": "eslint --cache src/ --fix --ext '.ts, .tsx'",
    "lint:staged": "lint-staged",
    "pretty": "prettier --write src/**/*.{ts,tsx}",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kundinos/react-hooks.git"
  },
  "keywords": [
    "react",
    "hooks"
  ],
  "author": {
    "name": "Yury Kundin",
    "email": "yury.kundin@yandex.ru"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kundinos/react-hooks/issues"
  },
  "homepage": "https://github.com/kundinos/react-hooks#readme",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{json,md,yml,ts,tsx}": "prettier --write",
    "*.(ts|tsx)": "eslint --cache --fix"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "@babel/runtime": "^7.13.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/react-dom": "^17.0.9",
    "@types/rollup-plugin-peer-deps-external": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^27.2.1",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^11.0.1",
    "prettier": "^2.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.57.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
