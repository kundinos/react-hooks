{
  "name": "@kundinos/react-hooks",
  "version": "0.10.0",
  "description": "Set of useful React hooks",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "compile": "rm -rf dist && rollup -c rollup.config.ts",
